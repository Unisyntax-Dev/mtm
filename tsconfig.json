{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "baseUrl": "./assets/src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "types": ["react", "react-dom"]
  },
  "include": ["assets/src/**/*"]
}
